# If you restore via restore_mssql_docker.sh, use localhost with the mapped port.
PHARMACYSTORE_SQL_SERVER=localhost,14333
PHARMACYSTORE_SQL_DATABASE=PharmacyStore
PHARMACYSTORE_SQL_USERNAME=sa
PHARMACYSTORE_SQL_PASSWORD=YOUR_PASSWORD
PHARMACYSTORE_SQL_DRIVER=ODBC Driver 18 for SQL Server
PHARMACYSTORE_SQL_ENCRYPT=false
PHARMACYSTORE_SQL_TRUST_CERT=true
PHARMACYSTORE_RANDOM_SEED=42
PHARMACYSTORE_RUN_WALK_FORWARD=true
PHARMACYSTORE_DATA_DIR=data
PHARMACYSTORE_ARTIFACTS_DIR=artifacts
